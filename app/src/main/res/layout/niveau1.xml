<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.redpanda.pandarezzo.MainActivity">


    <ImageButton
        android:id="@+id/re_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/re_bulle"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65" />


    <ImageButton
        android:id="@+id/mi_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/mi_bulle"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.846" />

    <ImageButton
        android:id="@+id/fa_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/fa_bulle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.848" />

    <ImageButton
        android:id="@+id/sol_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/sol_bulle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.652" />

    <ImageButton
        android:id="@+id/la_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/la_bulle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.445" />

    <ImageButton
        android:id="@+id/si_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/si_bulle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.781"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.182" />

    <ImageButton
        android:id="@+id/redpanda"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/redpanda"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.182" />

    <ImageButton
        android:id="@+id/do_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:layout_gravity="center"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:layout_weight="30"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/do_bulle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.838"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.444" />

</android.support.constraint.ConstraintLayout>

