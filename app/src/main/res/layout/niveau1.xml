<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.samuel.pandarezzo.MainActivity">


    <ImageButton
        android:id="@+id/re_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/re_bulle"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.692"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.287" />


    <ImageButton
        android:id="@+id/mi_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
        android:contentDescription="@string/abc_action_bar_home_description"
        android:scaleType="fitXY"
        android:src="@drawable/mi_bulle"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.607"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.783" />

    <ImageButton
        android:id="@+id/fa_bulle"
        android:layout_width="114dp"
        android:layout_height="76dp"
